{
  "name": "test-master-ai",
  "version": "1.0.0",
  "private": true,
  "description": "一款专注于验证不同提示词对大语言模型多次调用同一内容时输出结果差异的辅助工具",
  "main": "dist/main.js",
  "homepage": "./",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build && npm run build:electron",
    "build:vite": "vite build",
    "preview": "vite preview",
    "build:electron": "tsc -p tsconfig.electron.json",
    "pack": "electron-builder",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "admin:info": "node tools/admin-helper.js",
    "admin:reset": "node tools/reset-superadmin.js",
    "test:build": "node scripts/test-build.js",
    "generate:icons": "node scripts/generate-icons.js",
    "optimize:build": "node scripts/optimize-build.js"
  },
  "keywords": [
    "api-testing",
    "prompt-engineering",
    "llm",
    "electron",
    "react"
  ],
  "author": "hexun",
  "license": "GPL",
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@electron/rebuild": "3.2.13",
    "autoprefixer": "^10.4.21",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "concurrently": "^8.2.0",
    "electron": "^25.0.0",
    "electron-builder": "^24.0.0",
    "playwright": "^1.52.0",
    "postcss": "^8.5.4",
    "puppeteer": "^24.9.0",
    "typescript": "^5.0.0",
    "vite": "^5.2.0",
    "wait-on": "^7.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.0",
    "antd": "^5.8.0",
    "axios": "^1.5.0",
    "electron-store": "^8.1.0",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.5.2",
    "react-router-dom": "^7.6.1",
    "sql.js": "^1.13.0",
    "tailwindcss": "^3.3.0",
    "xlsx": "^0.18.5"
  },
  "build": {
    "appId": "com.apitestingtool.app",
    "productName": "test-master-ai",
    "directories": {
      "output": "release"
    },
    "compression": "store",
    "files": [
      "build/**/*",
      "dist/main.js",
      "node_modules/**/*",
      "!node_modules/.cache",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "!**/node_modules/playwright/**/*",
      "!**/node_modules/puppeteer/**/*"
    ],
    "asarUnpack": [
      "build/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "electron/icon.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64",
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "arm64",
            "x64"
          ]
        }
      ],
      "darkModeSupport": true,
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "identity": null,
      "signIgnore": ".*"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "electron/icon.ico",
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "API Testing Tool",
      "deleteAppDataOnUninstall": false
    },
    "dmg": {
      "title": "API Testing Tool",
      "iconSize": 80,
      "contents": [
        {
          "x": 150,
          "y": 150,
          "type": "file"
        },
        {
          "x": 400,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
